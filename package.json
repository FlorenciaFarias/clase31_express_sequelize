{
  "name": "express-database",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "clear && nodemon source/app.js -e js,mjs,json,ejs,css",
    "start": "node source/app.js",
    "sq-i": "sequelize init",
    "sq-ms": "sequelize init:migrations && sequelize init:seeders",
    "sq-mg": "sequelize migration:generate --name imagesProducts",
    "sq-sg": "sequelize seed:generate --name imagesProducts",
    "sq-db": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all"
  },
